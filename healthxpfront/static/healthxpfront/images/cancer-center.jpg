<svg xmlns="http://www.w3.org/2000/svg" width="600" height="400" viewBox="0 0 600 400">
  <rect width="600" height="400" fill="#f3e5f5" />
  
  <!-- Background Pattern -->
  <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
    <circle cx="20" cy="20" r="1" fill="#ce93d8" />
  </pattern>
  <rect width="600" height="400" fill="url(#grid)" />
  
  <!-- Main Elements -->
  <g transform="translate(300, 180)">
    <!-- Medical Caduceus Symbol -->
    <g transform="translate(-120, 0)" fill="none" stroke="#7b1fa2" stroke-width="4">
      <line x1="0" y1="-80" x2="0" y2="80" />
      <path d="M-30,-20 C-20,-60 20,-60 30,-20 C20,20 -20,20 -30,-20 Z" />
      <path d="M-30,20 C-20,-20 20,-20 30,20 C20,60 -20,60 -30,20 Z" />
    </g>
    
    <!-- DNA Strand -->
    <g transform="translate(80, 0)" stroke="#7b1fa2" stroke-width="3">
      <path d="M-30,-80 C30,-40 -30,0 30,40 C-30,80 30,120 -30,160" fill="none" />
      <path d="M30,-80 C-30,-40 30,0 -30,40 C30,80 -30,120 30,160" fill="none" />
      
      <!-- DNA Rungs -->
      <line x1="-20" y1="-70" x2="20" y2="-70" stroke-width="2" />
      <line x1="-25" y1="-50" x2="25" y2="-50" stroke-width="2" />
      <line x1="-28" y1="-30" x2="28" y2="-30" stroke-width="2" />
      <line x1="-25" y1="-10" x2="25" y2="-10" stroke-width="2" />
      <line x1="-20" y1="10" x2="20" y2="10" stroke-width="2" />
      <line x1="-25" y1="30" x2="25" y2="30" stroke-width="2" />
      <line x1="-28" y1="50" x2="28" y2="50" stroke-width="2" />
      <line x1="-25" y1="70" x2="25" y2="70" stroke-width="2" />
      <line x1="-20" y1="90" x2="20" y2="90" stroke-width="2" />
    </g>
    
    <!-- Microscope -->
    <g transform="translate(-20, -40)">
      <rect x="-30" y="50" width="60" height="20" fill="#9c27b0" rx="2" ry="2" />
      <rect x="-20" y="20" width="40" height="30" fill="#9c27b0" />
      <rect x="-15" y="-30" width="30" height="50" fill="#ba68c8" />
      <rect x="-5" y="-60" width="10" height="30" fill="#ba68c8" />
      <circle cx="0" cy="-60" r="8" fill="#9c27b0" />
      <rect x="-25" y="70" width="50" height="10" fill="#9c27b0" />
      <path d="M0,80 L-20,110 L20,110 Z" fill="#9c27b0" />
    </g>
  </g>
  
  <!-- Title -->
  <rect x="100" y="330" width="400" height="50" rx="10" ry="10" fill="#6a1b9a" />
  <text x="300" y="365" font-family="Arial" font-size="20" font-weight="bold" text-anchor="middle" fill="white">Cancer Care & Research Center</text>
</svg> 