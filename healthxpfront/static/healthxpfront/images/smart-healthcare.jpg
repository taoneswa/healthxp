<svg xmlns="http://www.w3.org/2000/svg" width="600" height="400" viewBox="0 0 600 400">
  <rect width="600" height="400" fill="#e8f5e9" />
  
  <!-- Background Circuit Pattern -->
  <g stroke="#81c784" stroke-width="1" fill="none">
    <path d="M0,50 L200,50 L200,150 L400,150 L400,250 L600,250" />
    <path d="M0,100 L150,100 L150,200 L300,200 L300,300 L600,300" />
    <path d="M100,0 L100,100 L300,100 L300,350 L450,350 L450,400" />
    <path d="M200,0 L200,350 L350,350 L350,250 L500,250 L500,400" />
    <path d="M300,0 L300,50 L400,50 L400,200 L550,200 L550,400" />
    
    <!-- Circuit Nodes -->
    <circle cx="200" cy="50" r="5" fill="#43a047" />
    <circle cx="200" cy="150" r="5" fill="#43a047" />
    <circle cx="400" cy="150" r="5" fill="#43a047" />
    <circle cx="400" cy="250" r="5" fill="#43a047" />
    <circle cx="150" cy="100" r="5" fill="#43a047" />
    <circle cx="150" cy="200" r="5" fill="#43a047" />
    <circle cx="300" cy="200" r="5" fill="#43a047" />
    <circle cx="300" cy="300" r="5" fill="#43a047" />
    <circle cx="100" cy="100" r="5" fill="#43a047" />
    <circle cx="300" cy="100" r="5" fill="#43a047" />
    <circle cx="300" cy="350" r="5" fill="#43a047" />
    <circle cx="450" cy="350" r="5" fill="#43a047" />
    <circle cx="200" cy="350" r="5" fill="#43a047" />
    <circle cx="350" cy="350" r="5" fill="#43a047" />
    <circle cx="350" cy="250" r="5" fill="#43a047" />
    <circle cx="500" cy="250" r="5" fill="#43a047" />
    <circle cx="300" cy="50" r="5" fill="#43a047" />
    <circle cx="400" cy="50" r="5" fill="#43a047" />
    <circle cx="400" cy="200" r="5" fill="#43a047" />
    <circle cx="550" cy="200" r="5" fill="#43a047" />
  </g>
  
  <!-- Smart City Elements -->
  <g transform="translate(300, 200)">
    <!-- Hospital Building -->
    <rect x="-90" y="-80" width="70" height="110" fill="#1b5e20" />
    <rect x="-80" y="-70" width="15" height="20" fill="#e8f5e9" />
    <rect x="-55" y="-70" width="15" height="20" fill="#e8f5e9" />
    <rect x="-80" y="-40" width="15" height="20" fill="#e8f5e9" />
    <rect x="-55" y="-40" width="15" height="20" fill="#e8f5e9" />
    <rect x="-80" y="-10" width="15" height="20" fill="#e8f5e9" />
    <rect x="-55" y="-10" width="15" height="20" fill="#e8f5e9" />
    <rect x="-75" y="30" width="40" height="15" fill="#1b5e20" stroke="#e8f5e9" stroke-width="2" />
    <path d="M-75,30 L-55,10 L-35,30" fill="#1b5e20" stroke="#e8f5e9" stroke-width="2" />
    <text x="-55" y="42" font-family="Arial" font-size="12" text-anchor="middle" fill="#e8f5e9">H</text>
    
    <!-- Digital Devices/IoT -->
    <g transform="translate(30, -30)">
      <!-- Tablet -->
      <rect x="-20" y="-20" width="40" height="60" rx="3" ry="3" fill="#2e7d32" stroke="#e8f5e9" stroke-width="2" />
      <rect x="-15" y="-15" width="30" height="45" fill="#a5d6a7" />
      <line x1="-5" y1="-5" x2="15" y2="-5" stroke="#2e7d32" stroke-width="2" />
      <line x1="-5" y1="5" x2="10" y2="5" stroke="#2e7d32" stroke-width="2" />
      <line x1="-5" y1="15" x2="15" y2="15" stroke="#2e7d32" stroke-width="2" />
      <rect x="-5" y="35" width="10" height="5" rx="2" ry="2" fill="#2e7d32" />
      
      <!-- Smartwatch -->
      <rect x="-35" y="25" width="20" height="25" rx="3" ry="3" fill="#2e7d32" stroke="#e8f5e9" stroke-width="2" />
      <rect x="-32" y="30" width="14" height="15" rx="1" ry="1" fill="#a5d6a7" />
      <path d="M-35,37 L-40,42 L-40,57 L-35,62" stroke="#2e7d32" stroke-width="3" fill="none" />
      <path d="M-15,37 L-10,42 L-10,57 L-15,62" stroke="#2e7d32" stroke-width="3" fill="none" />
      
      <!-- Smartphone -->
      <rect x="30" y="-10" width="20" height="40" rx="3" ry="3" fill="#2e7d32" stroke="#e8f5e9" stroke-width="2" />
      <rect x="33" y="-5" width="14" height="25" fill="#a5d6a7" />
      <circle cx="40" cy="25" r="3" fill="#e8f5e9" />
    </g>
    
    <!-- Data and Connectivity -->
    <g transform="translate(0, 0)">
      <circle cx="0" cy="0" r="30" fill="none" stroke="#388e3c" stroke-width="3" />
      <circle cx="0" cy="0" r="20" fill="none" stroke="#388e3c" stroke-width="2" />
      <circle cx="0" cy="0" r="10" fill="#388e3c" />
      
      <!-- Data Transfer Lines -->
      <line x1="-25" y1="-25" x2="-90" y2="-40" stroke="#43a047" stroke-width="2" stroke-dasharray="5,3" />
      <line x1="25" y1="-25" x2="70" y2="-30" stroke="#43a047" stroke-width="2" stroke-dasharray="5,3" />
      <line x1="25" y1="25" x2="60" y2="50" stroke="#43a047" stroke-width="2" stroke-dasharray="5,3" />
      <line x1="-25" y1="25" x2="-50" y2="50" stroke="#43a047" stroke-width="2" stroke-dasharray="5,3" />
    </g>
  </g>
  
  <!-- Title -->
  <rect x="100" y="330" width="400" height="50" rx="10" ry="10" fill="#388e3c" />
  <text x="300" y="365" font-family="Arial" font-size="20" font-weight="bold" text-anchor="middle" fill="white">Smart Healthcare & Innovation Park</text>
</svg> 