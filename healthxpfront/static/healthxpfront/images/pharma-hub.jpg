<svg xmlns="http://www.w3.org/2000/svg" width="600" height="400" viewBox="0 0 600 400">
  <rect width="600" height="400" fill="#e0f7fa" />
  
  <!-- Background Grid -->
  <g stroke="#b2ebf2" stroke-width="1">
    <path d="M0,50 L600,50 M0,100 L600,100 M0,150 L600,150 M0,200 L600,200 M0,250 L600,250 M0,300 L600,300 M0,350 L600,350" />
    <path d="M50,0 L50,400 M100,0 L100,400 M150,0 L150,400 M200,0 L200,400 M250,0 L250,400 M300,0 L300,400 M350,0 L350,400 M400,0 L400,400 M450,0 L450,400 M500,0 L500,400 M550,0 L550,400" />
  </g>
  
  <!-- Pills and Laboratory Items -->
  <g transform="translate(300, 200)">
    <!-- Lab Flask -->
    <path d="M-80,-50 L-60,50 L-140,50 Z" fill="#80deea" stroke="#00acc1" stroke-width="3" />
    <ellipse cx="-100" cy="50" rx="40" ry="10" fill="#80deea" stroke="#00acc1" stroke-width="3" />
    
    <!-- Pill Capsule -->
    <g transform="rotate(30)">
      <rect x="20" y="-15" width="80" height="30" rx="15" ry="15" fill="#ec407a" stroke="#c2185b" stroke-width="2" />
      <rect x="60" y="-15" width="40" height="30" rx="15" ry="15" fill="#f8bbd0" stroke="#c2185b" stroke-width="2" />
    </g>
    
    <!-- Test Tube -->
    <rect x="-20" y="-80" width="20" height="100" rx="10" ry="10" fill="#b2ebf2" stroke="#00acc1" stroke-width="2" />
    <rect x="-20" y="-10" width="20" height="30" rx="0" ry="0" fill="#00acc1" stroke="#00acc1" stroke-width="2" />
    
    <!-- Molecular Structure -->
    <g transform="translate(100, -60)" stroke="#0097a7" stroke-width="3" fill="#4dd0e1">
      <circle cx="0" cy="0" r="10" />
      <circle cx="30" cy="-20" r="10" />
      <circle cx="60" cy="0" r="10" />
      <circle cx="30" cy="20" r="10" />
      <line x1="0" y1="0" x2="30" y2="-20" />
      <line x1="30" y1="-20" x2="60" y2="0" />
      <line x1="60" y1="0" x2="30" y2="20" />
      <line x1="30" y1="20" x2="0" y2="0" />
    </g>
  </g>
  
  <!-- Title -->
  <rect x="150" y="330" width="300" height="50" rx="10" ry="10" fill="#00838f" />
  <text x="300" y="365" font-family="Arial" font-size="20" font-weight="bold" text-anchor="middle" fill="white">Pharmaceutical Innovation Hub</text>
</svg> 